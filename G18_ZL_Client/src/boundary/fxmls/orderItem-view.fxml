<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<StackPane prefHeight="110.0" prefWidth="251.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="40.0">
         <children>
            <ImageView fitHeight="109.0" fitWidth="97.0" pickOnBounds="true" preserveRatio="true" />
            <VBox alignment="CENTER" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="nameOrderLbl" text="name" VBox.vgrow="ALWAYS">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="amountLbl" text="X2">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Label fx:id="priceLbl" text="Label">
                     <font>
                        <Font size="15.0" />
                     </font>
                  </Label>
                  <Button fx:id="viewBtn" mnemonicParsing="false" text="View" visible="false" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</StackPane>
