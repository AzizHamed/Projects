<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="176.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.fxmlControllers.CartCustomItemControl">
   <children>
      <VBox prefHeight="196.0" prefWidth="294.0">
         <children>
            <HBox prefHeight="196.0" prefWidth="291.0">
               <children>
                  <VBox prefHeight="174.0" prefWidth="134.0">
                     <children>
                        <ImageView fitHeight="196.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </VBox>
                  <VBox alignment="TOP_CENTER" prefHeight="196.0" prefWidth="162.0">
                     <children>
                        <Label fx:id="priceLabel" prefHeight="18.0" prefWidth="39.0" scaleShape="false" text="11" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS" />
                        <Label fx:id="nameItemLabel" prefHeight="18.0" prefWidth="98.0" text="name">
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Label>
                        <HBox alignment="CENTER" prefHeight="165.0" prefWidth="162.0" spacing="10.0">
                           <children>
                              <Button fx:id="addBtn" mnemonicParsing="false" onMousePressed="#onAdditemPressed" text="+" />
                              <Label fx:id="amountLabel" text="1" />
                              <Button fx:id="deleteBtn" mnemonicParsing="false" onMousePressed="#onDeletePressed" text="-" />
                           </children>
                        </HBox>
                        <Button fx:id="viewItemBtn" mnemonicParsing="false" onMousePressed="#onViewItemPressed" text="view Item">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin>
                        </Button>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
