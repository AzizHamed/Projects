<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane prefHeight="176.0" prefWidth="297.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.fxmlControllers.CartItemControl">
   <children>
      <VBox prefHeight="196.0" prefWidth="294.0">
         <children>
            <HBox>
               <children>
                  <ImageView fitHeight="138.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </ImageView>
                  <VBox alignment="TOP_CENTER" spacing="20.0">
                     <children>
                        <Label fx:id="nameItemLabel" text="name">
                           <VBox.margin>
                              <Insets top="6.0" />
                           </VBox.margin>
                        </Label>
                        <Label fx:id="priceLabel" text="25" />
                        <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="142.0" spacing="10.0" VBox.vgrow="ALWAYS">
                           <children>
                              <Button fx:id="addBtn" mnemonicParsing="false" onMousePressed="#onAdditemPressed" text="+" />
                              <Label fx:id="amountLabel" text="1" />
                              <Button fx:id="deleteBtn" mnemonicParsing="false" onMousePressed="#onAddDeletePressed" text="-" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
