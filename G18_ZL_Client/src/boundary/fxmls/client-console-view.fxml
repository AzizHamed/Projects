<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane style="-fx-background-color: #FFF7F8;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="boundary.fxmlControllers.ClientConsoleController">
   <children>
      <VBox alignment="CENTER" StackPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" prefWidth="200.0" spacing="5.0" style="-fx-background-color: white;">
                     <children>
                        <ImageView fx:id="logoIV" fitHeight="84.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <Label fx:id="consoleLbl" alignment="CENTER" maxWidth="1.7976931348623157E308" text="Console Label" textAlignment="CENTER" wrapText="true">
                           <font>
                              <Font name="System Bold Italic" size="12.0" />
                           </font>
                        </Label>
                        <VBox fx:id="menuVB" alignment="CENTER" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                        </VBox>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Signed in as:" />
                        <Text fx:id="nameText" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
                           <font>
                              <Font name="System Bold" size="12.0" />
                           </font>
                        </Text>
                      <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" onMousePressed="#onLogOutPressed" style="-fx-background-color: #ff5000;" text="Log Out" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
                  <StackPane fx:id="mainSP" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: white;" HBox.hgrow="ALWAYS" />
               </children></HBox>
         </children>
      </VBox>
   </children>
</StackPane>
